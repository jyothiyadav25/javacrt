import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int a;
        a=s.nextInt();
        int  z=a%10;
        if(z<5)
        {
            System.out.println((a-z));
        }
        else
        {
            System.out.println(+a+(10-z));
        }
        }
}
